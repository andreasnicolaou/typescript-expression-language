{
    "name": "@andreasnicolaou/typescript-expression-language",
    "version": "1.0.1",
    "description": "TypeScript implementation of symfony/expression-language",
    "main": "dist/index.js",
    "private": false,
    "types": "dist/types/index.d.ts",
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "test": "jest",
        "format": "prettier --write .",
        "lint": "eslint . --ext .ts --fix"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/andreasnicolaou/typescript-expression-language.git"
    },
    "bugs": {
        "url": "https://github.com/andreasnicolaou/typescript-expression-language/issues"
    },
    "author": "Andreas Nicolaou",
    "license": "MIT",
    "dependencies": {
        "locutus": "^2.0.32",
        "lru-cache": "^11.0.2"
    },
    "jest": {
        "transform": {
            "^.+\\.(ts|tsx)$": [
                "ts-jest",
                {
                    "isolatedModules": true
                }
            ]
        },
        "testEnvironment": "node"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/locutus": "^0.0.8",
        "@types/node": "^20.3.1",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "esbuild": "^0.24.2",
        "eslint": "^8.43.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.2",
        "jest": "^29.7.0",
        "prettier": "3.4.2",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.3"
    },
    "keywords": [
        "symfony",
        "expression",
        "javascript",
        "language",
        "expression-language",
        "typescript",
        "andreasnicolaou"
    ]
}
