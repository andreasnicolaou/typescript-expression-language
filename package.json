{
  "name": "@andreasnicolaou/typescript-expression-language",
  "version": "1.1.2",
  "description": "TypeScript implementation of symfony/expression-language",
  "main": "dist/index.js",
  "private": false,
  "types": "dist/types/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write .",
    "lint": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andreasnicolaou/typescript-expression-language.git"
  },
  "bugs": {
    "url": "https://github.com/andreasnicolaou/typescript-expression-language/issues"
  },
  "author": {
    "name": "Andreas Nicolaou",
    "email": "anicolaou66@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "locutus": "^2.0.32",
    "lru-cache": "^11.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts)$": [
        "ts-jest"
      ]
    },
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/locutus": "^0.0.8",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "prettier": "3.5.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "symfony",
    "expression",
    "javascript",
    "language",
    "expression-language",
    "typescript",
    "andreasnicolaou"
  ]
}
