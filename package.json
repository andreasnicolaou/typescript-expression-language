{
  "name": "@andreasnicolaou/typescript-expression-language",
  "version": "1.1.0",
  "description": "TypeScript implementation of symfony/expression-language",
  "main": "dist/index.js",
  "private": false,
  "types": "dist/types/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write .",
    "lint": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andreasnicolaou/typescript-expression-language.git"
  },
  "bugs": {
    "url": "https://github.com/andreasnicolaou/typescript-expression-language/issues"
  },
  "author": "Andreas Nicolaou",
  "license": "MIT",
  "dependencies": {
    "locutus": "^2.0.32",
    "lru-cache": "^11.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "isolatedModules": true
        }
      ]
    },
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/locutus": "^0.0.8",
    "@types/node": "^22.13.4",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "esbuild": "^0.25.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "prettier": "3.5.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "keywords": [
    "symfony",
    "expression",
    "javascript",
    "language",
    "expression-language",
    "typescript",
    "andreasnicolaou"
  ]
}
